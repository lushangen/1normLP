load carsmall;
A = [Weight, Horsepower];
A = A(1:10,:);
b = MPG(1:10);
[data,info] = OneNormLP3033954135(A,b);
[y,fval] = linprog(b, [], [], A', zeros(size(A,2),1), -ones(size(A,1),1), ones(size(A,1),1));
disp(y);
disp(fval);

% Generate a random matrix of size nxn
n=6;
m=9;
A=randi([-n,m],m,n);
% C1=rand(n)
A(A==0)=n;
rank(A);
b=randi([-m,m],m,1);

%[m,n] = size(A);
flag = false;
B = [1:n];
B_bar = setdiff([1:m],B);
x=inv(A(B,:))*b(B); % same as x = (A(B,:))\b(B)
%x = (A(B,:))\b(B)
initx = x;
h = A*x - b;
A(B_bar,:)*x-b(B_bar);
y = ones(m,1);
y(B_bar) = sign(h(B_bar));
y(B) = -inv(A(B,:))' * A(B_bar,:)'* y(B_bar);


[data,info]=OneNormLP3033954135(A,b);
